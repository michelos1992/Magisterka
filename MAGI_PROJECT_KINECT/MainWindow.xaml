<Window x:Class="MAGI_PROJECT_KINECT.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MAGI_PROJECT_KINECT"
        mc:Ignorable="d"
        Title="MainWindow" Height="768" Width="1366" >

    <Grid Name="layoutGrid" Margin="10 0 10 0">
        <TextBox x:Name="textBox1" Height="47" TextWrapping="Wrap" Text="TextBox1" VerticalAlignment="Top" Width="120" Margin="130,10,1088,0"/>
        <TextBox x:Name="textBox2" Height="47" TextWrapping="Wrap" Text="TextBox2" VerticalAlignment="Top" Width="120" Margin="255,10,963,0"/>
        <TextBox x:Name="textBox3" Height="47" TextWrapping="Wrap" Text="TextBox3" VerticalAlignment="Top" Width="128" Margin="380,10,830,0"/>
        <TextBox x:Name="distanceTextBox" Height="47" Margin="5,10,1213,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="AnswerFromRobot" Height="27" TextWrapping="Wrap" Text="TextBox1" VerticalAlignment="Top" Margin="-3,93,1213,0"/>

        <Image Name ="MaskedColor" UseLayoutRounding="True" Margin="402,93,386,219" MouseLeftButtonDown="Window_MouseLeftButtonDown" MouseLeftButtonUp="Window_MouseLeftButtonUp" />
