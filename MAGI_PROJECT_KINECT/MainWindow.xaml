        <Button x:Name="Calibration" Content="Calibration" HorizontalAlignment="Left" Margin="-3,266,0,0" VerticalAlignment="Top" Width="128" Height="42" Click="Calibration_Click" />
        <Button x:Name="CheckedButton" Content="Check" HorizontalAlignment="Left" Margin="130,266,0,0" VerticalAlignment="Top" Width="128" Height="42" Click="CheckedButton_Click" />
        <Button x:Name="proba_Copy" Content="ScanFully" HorizontalAlignment="Left" Margin="130,313,0,0" VerticalAlignment="Top" Width="128" Height="42" Click="SpaceScanFully_Click" />
        <Button x:Name="ConnectToRobot" Content="Connect robot" HorizontalAlignment="Left" Margin="529,10,0,0" VerticalAlignment="Top" Width="76" Click="ConnectToRobot_Click"/>
        <Button x:Name="ColorCamera" Content="Color Camera Window-View" HorizontalAlignment="Left" Margin="529,35,0,0" VerticalAlignment="Top" Width="157" Click="ColorCamera_Click" />
        <Button x:Name="ConnectRobot" Content="ConnectWithRobot" HorizontalAlignment="Left" Margin="-3,219,0,0" VerticalAlignment="Top" Width="128" Height="42" Click="ConnectRobot_Click" />
        <Button x:Name="Gogo" Content="Gogogo" HorizontalAlignment="Left" Margin="-3,172,0,0" VerticalAlignment="Top" Width="128" Height="42" Click="Gogo_Click"  />
        <Button x:Name="Disconnect" Content="Disconnect" HorizontalAlignment="Left" Margin="130,219,0,0" VerticalAlignment="Top" Width="128" Height="42" Click="Disconnect_Click"  />
        <Button x:Name="STOP" Content="STOP" HorizontalAlignment="Left" Margin="130,172,0,0" VerticalAlignment="Top" Width="128" Height="42" Click="STOP_Click"  />

        <Label Content="Odpowiedz od robota" HorizontalAlignment="Left" Margin="-3,67,0,0" VerticalAlignment="Top" Width="128"/>
        <Label Content="sprawdzone roznice" HorizontalAlignment="Left" Margin="516,55,0,0" VerticalAlignment="Top" Width="119"/>
        <Label Content="--wpisz ilosc km" HorizontalAlignment="Left" Margin="130,94,0,0" VerticalAlignment="Top" Width="128"/>

    </Grid>
